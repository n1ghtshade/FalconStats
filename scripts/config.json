{
  "apc": false,
  "zpool": false,
  "diskspace": {
    "use": true,
    "mountpoints": [
      "/",
      "/boot"
    ]
  },
  "hddtemp": {
    "use": true,
    "drives": {
      "0": "/dev/sda"
    }
  },
  "service": {
    "use": true,
    "services": {
      "0": "pm2",
      "1": "samba"
    }
  },
  "ssl": false
}